{
  "name": "plate-scanner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Development tooling for PassingPlates web app.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
