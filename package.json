  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^24.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@tesseract.js-data/eng": "^1.0.0",
    "onnxruntime-web": "^1.22.0",
    "tesseract.js": "^5.1.1"
  }
}
